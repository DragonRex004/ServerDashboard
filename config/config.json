{
  "application": {
    "name": "Server Dashboard",
    "version": "1.0.0",
    "port": 7070,
    "environment": "development"
  },
  "security": {
    "session_timeout": 3600,
    "max_login_attempts": 5,
    "password_min_length": 4
  },
  "admin": {
    "username": "admin",
    "password": "admin123",
    "email": "admin@serverdashboard.local",
    "role": "administrator",
    "created_at": "2024-01-01T00:00:00Z",
    "permissions": [
      "user_management",
      "system_configuration",
      "server_monitoring",
      "dashboard_access"
    ]
  },
  "default_users": [
    {
      "username": "user",
      "password": "user123",
      "email": "user@serverdashboard.local",
      "role": "user",
      "permissions": [
        "dashboard_access",
        "server_monitoring"
      ]
    }
  ],
  "database": {
    "auto_migrate": true,
    "backup_enabled": true,
    "backup_interval_hours": 24
  },
  "logging": {
    "level": "INFO",
    "file_enabled": true,
    "console_enabled": true
  },
  "features": {
    "user_registration": false,
    "password_reset": true,
    "two_factor_auth": false
  }
}
